{
  "name": "BrandNewTab",
  "description": "AA chrome extension to replace and simplify the new tab",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icons/48.png"
  },
  "icons": {
    "16": "icons/48.png",
    "48": "icons/48.png",
    "128": "icons/192.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}