{
  "name": "BrandNewTab",
  "description": "Change new tab with beautiful images, gradients, or patterns",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icons/48.png"
  },
  "icons": {
    "16": "icons/48.png",
    "48": "icons/48.png",
    "128": "icons/192.png"
  },
  "chrome_url_overrides": {
    "newtab": "brandnewtab.html"
  },
  "permissions": [
    "activeTab"
  ]
}