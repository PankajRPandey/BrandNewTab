{
  "name": "BrandNewTab",
  "description": "A new tab.",
  "version": "1.0",
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon20.png"
  },
  "chrome_url_overrides": {
	"newtab":"newtab.html"
  },
"permissions": ["activeTab"]
}